{
  "dashboard": {
    "id": null,
    "title": "Neural Marketing Consciousness Dashboard",
    "tags": ["neural", "marketing", "ai", "consciousness"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Consciousness Level Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "neural_consciousness_level",
            "legendFormat": "Level {{level}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Content Generation Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(content_generations_total[5m])",
            "legendFormat": "Generations/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Generations per second",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "AI API Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "openai_api_requests_total",
            "legendFormat": "API Requests"
          },
          {
            "expr": "openai_api_tokens_used_total",
            "legendFormat": "Tokens Used"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "User Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          },
          {
            "expr": "new_users_total",
            "legendFormat": "New Users"
          }
        ],
        "yAxes": [
          {
            "label": "Users",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "System Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "node_memory_MemAvailable_bytes",
            "legendFormat": "Available Memory"
          },
          {
            "expr": "node_cpu_seconds_total",
            "legendFormat": "CPU Usage"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes/Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Neural Insights Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_insights_generated_total",
            "legendFormat": "Total Insights"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Average Consciousness Level",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(neural_consciousness_level)",
            "legendFormat": "Avg Level"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 30
                },
                {
                  "color": "green",
                  "value": 70
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
  }
}

