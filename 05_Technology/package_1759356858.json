{
  "name": "ai-marketing-feedback-system",
  "version": "2.0.0",
  "description": "Sistema avanzado de integración y análisis de feedback de clientes para IA Marketing en LATAM",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t ai-marketing-feedback .",
    "docker:run": "docker run -p 3001:3001 ai-marketing-feedback",
    "docker:compose": "docker-compose up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "health": "curl http://localhost:3001/health",
    "docs": "typedoc src --out docs",
    "clean": "rm -rf dist node_modules/.cache",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run prisma:generate"
  },
  "keywords": [
    "ai",
    "marketing",
    "feedback",
    "analytics",
    "sentiment-analysis",
    "machine-learning",
    "latin-america",
    "customer-insights",
    "real-time",
    "websocket",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "Adan - AI Marketing Expert",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ws": "^8.18.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "redis": "^4.6.12",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "pdfkit": "^0.14.0",
    "exceljs": "^4.4.0",
    "sharp": "^0.33.0",
    "jimp": "^0.22.10",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.0",
    "chartjs-node-canvas": "^4.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-validator": "^6.15.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typedoc": "^0.25.4",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adan/ai-marketing-feedback-system.git"
  },
  "bugs": {
    "url": "https://github.com/adan/ai-marketing-feedback-system/issues"
  },
  "homepage": "https://github.com/adan/ai-marketing-feedback-system#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts,js,json",
    "ignore": ["src/**/*.test.ts"],
    "exec": "ts-node-dev --respawn --transpile-only src/server.ts"
  }
}