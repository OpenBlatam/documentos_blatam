const financialDashboard = {
    // MÃ©tricas de Liquidez
    liquidez: {
        corriente: 2.4,
        acida: 1.7,
        absoluta: 0.6,
        tendencia: '+5%',
        alerta: 'verde'
    },
    
    // MÃ©tricas de Rentabilidad
    rentabilidad: {
        margenBruto: 63.8,
        margenNeto: 14.6,
        roa: 31.9,
        roe: 53.7,
        roiMarketing: 420,
        tendencia: '+12%',
        alerta: 'verde'
    },
    
    // MÃ©tricas de Endeudamiento
    endeudamiento: {
        deudaActivos: 40.5,
        deudaPatrimonio: 68.1,
        coberturaIntereses: 7.4,
        tendencia: '-8%',
        alerta: 'verde'
    },
    
