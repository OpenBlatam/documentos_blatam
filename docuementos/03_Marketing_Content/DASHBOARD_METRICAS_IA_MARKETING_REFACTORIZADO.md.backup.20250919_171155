    iaMarketing: {
        automatizacion: 75,
        precisionTargeting: 89,
        conversionLeads: 3.2,
        retencionClientes: 78,
        eficienciaOperativa: 86,
        tendencia: '+18%',
        alerta: 'verde'
    }
};

// FunciÃ³n de alertas automÃ¡ticas
function checkFinancialAlerts() {
    const alerts = [];
    
    if (financialDashboard.liquidez.corriente < 1.5) {
        alerts.push('ğŸ”´ CRÃTICO: Liquidez corriente muy baja');
    }
    
    if (financialDashboard.rentabilidad.roiMarketing < 200) {
        alerts.push('ğŸŸ¡ ATENCIÃ“N: ROI Marketing por debajo del objetivo');
    }
    
    if (financialDashboard.endeudamiento.deudaPatrimonio > 80) {
        alerts.push('ğŸ”´ CRÃTICO: Endeudamiento excesivo');
    }
    
    return alerts;
}
