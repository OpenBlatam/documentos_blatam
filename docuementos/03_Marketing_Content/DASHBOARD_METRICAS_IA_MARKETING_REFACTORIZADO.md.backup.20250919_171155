// DASHBOARD FINANCIERO EN TIEMPO REAL
// ===================================

const financialDashboard = {
    // MÃ©tricas de Liquidez
    liquidez: {
        corriente: 2.4,
        acida: 1.7,
        absoluta: 0.6,
        tendencia: '+5%',
        alerta: 'verde'
    },
    
    // MÃ©tricas de Rentabilidad
    rentabilidad: {
        margenBruto: 63.8,
        margenNeto: 14.6,
        roa: 31.9,
        roe: 53.7,
        roiMarketing: 420,
        tendencia: '+12%',
        alerta: 'verde'
    },
    
    // MÃ©tricas de Endeudamiento
    endeudamiento: {
        deudaActivos: 40.5,
        deudaPatrimonio: 68.1,
        coberturaIntereses: 7.4,
        tendencia: '-8%',
        alerta: 'verde'
    },
    
    // MÃ©tricas de IA Marketing
    iaMarketing: {
        automatizacion: 75,
        precisionTargeting: 89,
        conversionLeads: 3.2,
        retencionClientes: 78,
        eficienciaOperativa: 86,
        tendencia: '+18%',
        alerta: 'verde'
    }
};

// FunciÃ³n de alertas automÃ¡ticas
function checkFinancialAlerts() {
    const alerts = [];
    
    if (financialDashboard.liquidez.corriente < 1.5) {
        alerts.push('ğŸ”´ CRÃTICO: Liquidez corriente muy baja');
    }
    
    if (financialDashboard.rentabilidad.roiMarketing < 200) {
        alerts.push('ğŸŸ¡ ATENCIÃ“N: ROI Marketing por debajo del objetivo');
    }
    
    if (financialDashboard.endeudamiento.deudaPatrimonio > 80) {
        alerts.push('ğŸ”´ CRÃTICO: Endeudamiento excesivo');
    }
    
    return alerts;
}
```

