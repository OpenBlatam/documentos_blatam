{
  "name": "ai-marketing-feedback-system",
  "version": "1.0.0",
  "description": "Advanced Customer Feedback Integration System for AI Marketing in LATAM",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t ai-marketing-feedback-system .",
    "docker:run": "docker run -p 3001:3001 ai-marketing-feedback-system",
    "docker:compose": "docker-compose up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/scripts/seed.ts"
  },
  "keywords": [
    "AI",
    "Marketing",
    "Feedback",
    "LATAM",
    "SaaS",
    "Node.js",
    "TypeScript",
    "Analytics",
    "Machine Learning",
    "Emotional Intelligence",
    "Cultural Analysis",
    "Predictive Metrics",
    "WebSocket",
    "Real-time",
    "Consciousness"
  ],
  "author": "Adan - AI Marketing Expert",
  "license": "MIT",
